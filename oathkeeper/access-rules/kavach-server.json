[
  {
    "id": "kavach:server:protected",
    "upstream": {
      "preserve_host": true,
      "url": "http://kavach-server:8000",
      "strip_path": ".factly/kavach/server"
    },
    "match": {
      "url": "http://127.0.0.1:4455/.factly/kavach/server/<.*>",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    },
    "authenticators": [
      {
        "handler": "cookie_session"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [
      {
        "handler": "id_token"
      },
      {
        "handler": "hydrator"
      },
      {
        "handler": "header"
      }
    ]
  }
]