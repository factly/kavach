{
  "name": "identity",
  "version": "0.3.0",
  "description": "UI for identity",
  "private": true,
  "license": "MIT",
  "cacheDirectories": [
    "node_modules"
  ],
  "dependencies": {
    "@codemirror/lang-html": "^0.19.4",
    "@codemirror/lang-json": "^0.19.2",
    "@codemirror/theme-one-dark": "^0.19.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@uiw/react-codemirror": "^4.11.4",
    "@uppy/aws-s3": "^1.7.3",
    "@uppy/core": "^1.14.0",
    "@uppy/google-drive": "^1.5.18",
    "@uppy/image-editor": "^0.1.8",
    "@uppy/react": "^1.10.10",
    "@uppy/url": "^1.5.13",
    "@ant-design/pro-layout": "^7.2.6",
    "antd": "^5.0.4",
    "dayjs": "^1.11.6",
    "axios": "^0.21.4",
    "deep-equal": "^2.0.5",
    "js-cookie": "^2.2.1",
    "posthog-js": "^1.57.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^4.9.0",
    "react-lottie": "^1.2.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.3.0",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "tests:e2e": "playwright test --config=e2e-tests.config.js --project=Webkit",
    "eject": "react-scripts eject",
    "format:check": "prettier --check \"**/*.{js,jsx,yml,yaml,json,css,scss,md}\"",
    "format": "prettier --write \"**/*.{js,jsx,yml,yaml,json,css,scss,md,mjs}\""
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,yml,yaml,json,css,scss,md}": [
      "npm run format",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^17.0.3",
    "@playwright/test": "^1.17.2",
    "cz-conventional-changelog": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "enzyme-to-json": "^3.6.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "redux-mock-store": "^1.5.4",
    "semantic-release": "^19.0.3"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ]
  },
  "overrides": {
    "@codemirror/state": "6.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
