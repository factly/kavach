{
  "name": "kavach-studio",
  "version": "0.3.0",
  "description": "kavach adminisration UI",
  "private": true,
  "license": "MIT",
  "cacheDirectories": [
    "node_modules"
  ],
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@uppy/aws-s3": "^1.7.3",
    "@uppy/core": "^1.14.0",
    "@uppy/google-drive": "^1.5.18",
    "@uppy/image-editor": "^0.1.8",
    "@uppy/react": "^1.10.10",
    "@uppy/url": "^1.5.13",
    "antd": "^4.16.13",
    "antd-theme-webpack-plugin": "^1.3.9",
    "axios": "^0.21.4",
    "moment": "^2.26.0",
    "playwright": "^1.17.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "5.0.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "tests:e2e": "playwright test --config=e2e-tests.config.js --project=Webkit",
    "eject": "react-scripts eject",
    "format:check": "prettier --check \"**/*.{js,jsx,yml,yaml,json,css,scss,md}\"",
    "format": "prettier --write \"**/*.{js,jsx,yml,yaml,json,css,scss,md,mjs}\""
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,yml,yaml,json,css,scss,md}": [
      "npm run format",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^9.1.2",
    "@playwright/test": "^1.17.2",
    "cz-conventional-changelog": "^3.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "enzyme-to-json": "^3.6.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "redux-mock-store": "^1.5.4",
    "semantic-release": "^17.1.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
